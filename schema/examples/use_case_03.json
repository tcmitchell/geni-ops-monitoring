  'id': 'bbn-ig',
  'selfRef': 'https://datastore.instageni.gpolab.bbn.com/domains/bbn-ig',
  'urn': 'urn:publicid:IDN+instageni.gpolab.bbn.com+authority+cm',
  'ts': 1391192685740849,
