[main]
<% if database_type == 'mysql' -%>
dbtype: mysql
<% else -%>
dbtype: postgres
<% end -%>

[postgres]
database: local
username: localstore
password: <%= postgres_localstore_password %>
host: localhost
port: 5432

[mysql]
database: local
username: localstore
password: <%= mysql_localstore_password %>
host: localhost
port: 3306
